<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://localhost/Prediction</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>localhost</MSDeployServiceURL>
    <DeployIisAppPath>Default Web Site/Prediction</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>InProc</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="PredictionEntities" Order="1" Enabled="False">
          <Destination Path="Data Source=CSIE;Initial Catalog=Prediction;Integrated Security=True;Application Name=EntityFramework;Context Connection=False" Name="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="PredictionAPI.Models.PredictionEntities, PredictionAPI" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)PredictionEntities-Web.config Connection String">
      <ParameterValue>metadata=res://*/Models.Prediction.csdl|res://*/Models.Prediction.ssdl|res://*/Models.Prediction.msl;provider=System.Data.SqlClient;provider connection string="Data Source=CSIE;Initial Catalog=Prediction;Integrated Security=True;Application Name=EntityFramework;Context Connection=False"</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>